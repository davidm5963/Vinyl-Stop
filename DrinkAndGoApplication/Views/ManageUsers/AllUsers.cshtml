@model UserListViewModel
<div class="container">
    <div style="padding-top: 40px" class="row">
        <div class="col-lg-12">
            <div class="main-box clearfix">
                <div class="table-responsive">
                    <table class="table -list">
                        <thead>
                            <tr>
                                <th><span>User</span></th>
                                <th><span>Email</span></th>
                                <th class="text-center"><span>Phone Number</span></th>
                                <th>&nbsp;</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var userVm in @Model.Users)
                            {
                                <tr>
                                    <td class="table-data">
                                        <a href="#"><h4>@userVm.User.FirstName @userVm.User.LastName</h4></a>
                                        <span class="text-muted">@userVm.UserRoles[0]</span>
                                    </td>
                                    <td class="table-data">
                                        @userVm.User.Email
                                    </td>
                                    <td class="text-center table-data">
                                        @userVm.User.PhoneNumber
                                    </td>
                                    <td style="width: 20%;"  class="table-data">
                                        <a href="#" class="table-link">
                                            <span class="fa-stack">
                                                <i class="fa fa-square fa-stack-2x"></i>
                                                <i class="fa fa-pencil fa-stack-1x fa-inverse"></i>
                                            </span>
                                        </a>
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

